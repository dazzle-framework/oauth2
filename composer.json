{
    "name": "dazzlesoftware/oauth2",
    "type": "library",
    "description": "Dazzle OAuth2 Package",
    "keywords": ["dazzle", "framework", "oauth2"],
    "homepage": "https://github.com/dazzle-framework/oauth2",
    "license": "GPL-3.0-or-later",
    "require": {
       "php": "^7.3.0",
        "dazzlesoftware/application": "^1.0|^2.0",
        "dazzlesoftware/http": "^1.0|^2.0",
        "dazzlesoftware/input": "^1.0|^2.0",
        "dazzlesoftware/session": "^1.0|^2.0",
        "dazzlesoftware/uri": "^1.0|^2.0"
    },
    "require-dev": {
        "dazzlesoftware/dazzle-coding-standard": "^1.0|^2.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Dazzle\\OAuth2\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dazzle\\OAuth2\\UnitTests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev",
            "dev-develop": "2.x-dev"
        }
    }
}
